<!-- Modal -->

  
      <div class="modal-body">
        You will be logged out due to inactivity in <span id="ticker">{{ countdown }}</span> seconds. Press the button below to continue
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" {{ action decline target="view" }}>Close</button>
        <button type="button" class="btn btn-primary" {{ action continue target="view" }}>Save changes</button>
      </div>
    
